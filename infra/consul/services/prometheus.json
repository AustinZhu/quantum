{
  "service": {
    "id": "prometheus",
    "name": "prometheus",
    "address": "prometheus",
    "port": 9090,
    "tags": ["infra", "observability", "metrics"],
    "checks": [
      {
        "id": "prometheus-http",
        "name": "Prometheus HTTP",
        "http": "http://prometheus:9090/-/healthy",
        "interval": "15s",
        "timeout": "5s"
      }
    ]
  }
}
