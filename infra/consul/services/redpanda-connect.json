{
  "service": {
    "id": "redpanda-connect",
    "name": "redpanda-connect",
    "address": "redpanda-connect",
    "port": 4195,
    "tags": ["infra", "streaming", "processor"],
    "checks": [
      {
        "id": "redpanda-connect-tcp",
        "name": "Redpanda Connect TCP",
        "tcp": "redpanda-connect:4195",
        "interval": "10s",
        "timeout": "3s"
      },
      {
        "id": "redpanda-connect-http-ready",
        "name": "Redpanda Connect HTTP Ready",
        "http": "http://redpanda-connect:4195/ready",
        "interval": "10s",
        "timeout": "3s"
      }
    ]
  }
}
